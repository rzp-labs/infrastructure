# Cloudflare Configuration
CF_DNS_API_TOKEN=your_cloudflare_dns_api_token_here

# Traefik ACME Email
ACME_EMAIL=your_email@example.com

# Domain Configuration
DOMAIN=example.com
ZITADEL_PUBLIC_DOMAIN=auth.example.com

# Timezone
TZ=America/New_York

# OAuth2 Proxy (Zitadel)
TRAEFIK_OAUTH_CLIENT_ID=your_zitadel_client_id
TRAEFIK_OAUTH_CLIENT_SECRET=your_zitadel_client_secret
TRAEFIK_OAUTH_COOKIE_SECRET=replace-with-32-byte-base64
TRAEFIK_OAUTH_ALLOWED_EMAIL_DOMAIN=example.com
